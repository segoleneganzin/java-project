<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="439.0" prefWidth="546.0"
	stylesheets="@style.css" xmlns="http://javafx.com/javafx/19"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.HorairesController">
	<children>
		<Label alignment="CENTER" contentDisplay="CENTER"
			layoutX="109.0" layoutY="28.0" prefHeight="36.0" prefWidth="406.0"
			styleClass="titrePage" text="Horaires de la piscine">
			<font>
				<Font size="25.0" />
			</font>
		</Label>
		<ImageView fitHeight="87.0" fitWidth="109.0" layoutY="2.0"
			styleClass="logoEntete">
			<image>
				<Image url="@../Images/logo_remi.png" />
			</image>
		</ImageView>
		<Button fx:id="horairesRetourAcc" layoutX="17.0"
			layoutY="400.0" mnemonicParsing="false" onAction="#retourAcc"
			styleClass="boutonRetour" text="Retour" />
		<Text layoutX="188.0" layoutY="191.0" strokeType="OUTSIDE"
			strokeWidth="0.0" style="-fx-font-weight: bold;"
			text="Horaire d'ouverture :" underline="true" />
		<Label fx:id="horaireOuv" layoutX="323.0" layoutY="178.0"
			text="Label" />
		<Label fx:id="horaireFerm" layoutX="331.0" layoutY="216.0"
			text="Label" />
		<Text layoutX="188.0" layoutY="229.0" strokeType="OUTSIDE"
			strokeWidth="0.0" style="-fx-font-weight: bold;"
			text="Horaire de fermeture :" underline="true" />
	</children>
</AnchorPane>